{
  "name": "lattice-lottery-plus",
  "version": "0.2.0-0",
  "description": "基于 Vue3 的抽奖组件",
  "main": "./lib/lattice-lottery.umd.min.js",
  "private": false,
  "license": "MIT",
  "files": [
    "lib/*",
    "README.md",
    "packages/*",
    "package.json"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
